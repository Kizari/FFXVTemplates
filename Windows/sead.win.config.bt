              ushort structSize;
              ushort numZeroones;
              Count = numZeroones;
              ushort reserved2;
              unsigned int reserved3[2];
            }HEADER <open=true>;
            break;
        default:
            ubyte version;
            ubyte reserved;
            short structSize;
            ushort count;
            Count = count;
            ushort reserved2;
            unsigned int reserved3[2];
            break;
    }           
    local int z<hidden=true> = 0;
    local int x<hidden=true> = 0;
    local int test<hidden=true> = 0;
    if(Count){
        int loc[Count];
        for(z=0;z<Count;z++){
            FSeek(loc[z] + s.offset);
            switch(s.id){                
                case ("ctgr"): //Category
                    test = ReadByte();
