                ubyte data[commandSize];
            }CATEGORY_VOLUME_COMMAND_V2<open=true>;
        }
        break;
      case CONFIG_EVENT_COMMAND_CATEGORY_PITCH:
        Assert(commandSize == 12);
        struct CONFIG_EVENT_CATEGORY_PITCH_COMMAND {
            float pitch;
            float fadeTime;
            ushort categoryNumber;
            ushort reserved;
        } CATEGORY_PITCH_COMMAND<open=true>;
        break;
      case CONFIG_EVENT_COMMAND_CATEGORY_SUSPEND:
        Assert(commandSize == 8);
       struct CONFIG_EVENT_CATEGORY_SUSPEND_COMMAND{
            float fadeTime;
            ushort categoryNumber;
            ushort reserved;
        }CATEGORY_SUSPEND_COMMAND<open=true>;
        break;
      case CONFIG_EVENT_COMMAND_CATEGORY_RESUME: 
        Assert(commandSize == 8);
        struct CONFIG_EVENT_CATEGORY_RESUME_COMMAND{
            float fadeTime;
            ushort categoryNumber;
            ushort reserved;
        }CATEGORY_RESUME_COMMAND<open=true>;
        break;
      case CONFIG_EVENT_COMMAND_CATEGORY_STOP:
        Assert(commandSize == 8);
        struct CONFIG_EVENT_CATEGORY_STOP_COMMAND
        {
          float fadeTime;
          ushort categoryNumber;
          ushort reserved;
        }CATEGORY_STOP_COMMAND<open=true>;
        break;
      case CONFIG_EVENT_COMMAND_CATEGORY_FIXED_VOLUME:
        Assert(commandSize == 4);
        struct CONFIG_EVENT_CATEGORY_FIXED_VOLUME_COMMAND
        {
          ushort categoryNumber;
          ubyte enableOverride;
          ubyte fixedVolumeValue;
        }CATEGORY_FIXED_VOLUME_COMMAND<open=true>;
        break;
      case CONFIG_EVENT_COMMAND_CATEGORY_FIXED_POSITION:
        Assert(commandSize == 4);
        struct CONFIG_EVENT_CATEGORY_FIXED_POSITION_COMMAND
        {
          ushort categoryNumber;
          ubyte enableOverride;
          ubyte fixedPositionValue;
        }CATEGORY_FIXED_POSITION_COMMAND<open=true>;
        break;
      case CONFIG_EVENT_COMMAND_EFFECT_PRESET:
        Assert(commandSize == 8);
        struct CONFIG_EVENT_EFFECT_PRESET_COMMAND
        {
          short presetNumber;
          ushort reserved;
          float fadeTime;
        }EVENT_EFFECT_PRESET_COMMAND<open=true>;
        break;
      case CONFIG_EVENT_COMMAND_EFFECT_PARAM:
        Assert(commandSize == 16);
        struct CONFIG_EVENT_EFFECT_PARAM_COMMAND
        {
          ushort busNumber;
          ubyte slotNumber;
          ubyte reserved1;
          ushort paramType;
          ushort reserved2;
          float param;
          float fadeTime;
        }EVENT_EFFECT_PARAM_COMMAND<open=true>;
        break;
      default:
        Assert(0);
        break;
  }
}CONFIG_EVENT_COMMAND_HEADER <open=true>;

//section reads
typedef struct (CONFIGCHUNKTABLEELEMENT &s) {
    FSeek(s.offset);
    local string name<hidden=true> = s.id;
    local int Count<hidden=true> = 0;
    switch(s.id){
