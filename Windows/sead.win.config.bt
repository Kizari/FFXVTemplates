//basic align function
void Align(int n) {
    local int value = FTell() % n;

    if (value) {
        FSkip(n - value);
    }
};

//for aligning with a fixed string to inculde 0 byte
void AlignS(int n) {
    local int value = FTell() % n;

    if (value) {
        FSkip(n - value);
    } else {
        FSkip(n);
    }
};

