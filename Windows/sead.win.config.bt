                    {
                      ubyte version;
                      CONFIG_PLATFORM_EXTRA_SETTINGS_TYPES type;
                      ushort structSize;
                      unsigned int reserved[3];
                      if(type == CONFIG_PLATFORM_EXTRA_SETTINGS_IPHONE_DEVICE_FILTER){
                          struct CONFIGPLATFORMEXTRASETTINGS_IPHONEDEVICESPEAKERFILTERSETTINGS
                          {
                              ubyte version;
                              ubyte devicenamelen;
                              ushort reserved;
                              int filterType;
                              float frequency;
                              float q;
                              float gain;
                              uint reserved2[3];
                              char name[devicenamelen];
                          }IPHONEDEVICESPEAKERFILTERSETTINGS<open=true>;
                      }
                    }FORMEXTRASETTINGSHEADER<open=true>;
                    break;
                case ("zeon"): //ZeroOne never seen
                    struct CONFIGZEROONEHEADER
                    {
                      ubyte version;
                      ubyte useParamNamelen;
                      ushort structSize;
                      ushort numPoints;
                      ushort reserved1;
                      float minRange;
                      float maxRange;
                      //gets messed up here, not sure as I haven't seen it used
                      
                      //busSend
                      ubyte number;
                      ubyte target;
                      ushort reserved1;
                      unsigned int reserved2[3];
                      
                      //busEffect
                      ubyte number;
                      ubyte targetSlot;
                      ubyte reserved1[2];
                      uint targetParam;
                      unsigned int reserved2[2];
                      
                      //category
                      ubyte number;
                      ubyte targetParam;
                      ushort reserved1;
                      unsigned int reserved2[3];
  
                      ubyte target;
                      ubyte reserved2[3];
                      uint reserved3[3];
                      if(numPoints){
                        struct  CONFIGZEROONEPOINT
                        {
                          float zeroone;
                          float value;
                          ubyte curve;
                          ubyte reserved1;
                          ushort reserved2;
                          uint reserved3;
                        }ZEROONEPOINT[numPoints]<open=true>;
                      }
                    };
                    break;
                default:
                    break;
            }
        }
    }
} SectionData <bgcolor=0x00FF00>;

//file headers
struct CONFIGFILEHEADER{
    LittleEndian();
    char id[4]<bgcolor=0x0000FF>;
    Assert(id =="scnf");
    byte version;
    byte reserved;
    ushort structSize;
    ubyte numChunks;
    ubyte reserved2;
    ushort reserved3;
    uint fileSize;
    local int i<hidden=true> = 0;
    for(i=0;i<numChunks;i++){
        CONFIGCHUNKTABLEELEMENT Section(i)<name="CONFIGCHUNKTABLEELEMENT", read=secrName, bgcolor=bcolors[i]>;
    }
    for(i=0;i<numChunks;i++){
        SectionData SecData(Section[i])<read=secName, bgcolor=bcolors[i]>;
    }
} CONFIGFILE<open=true>;

//entry name display reads
string secName(SectionData &t) {
    return t.name;
};

string secrName(CONFIGCHUNKTABLEELEMENT &t) {
    return t.id;
};

string PRESETPACKStr(CONFIGEFFECTPRESETPACKHEADER &t) {
    return t.name;
};

string BUSName(BUS_Read &t) {
    return t.name;
};

string CTGRName(CONFIGCATEGORY &t) {
    return t.name;
};

string EVTName(CONFIG_EVENT_HEADER &t) {
    return t.name;
};

string CTGLName(CONFIGCATEGORYLAYER &t) {
    return t.name;
};
