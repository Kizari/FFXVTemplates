            } HEADER <open=true>;
            break;
        case "pext": //PlatformExtra
            struct CONFIGPLATFORMEXTRASETTINGSCHUNKHEADER
            {
              ubyte version;
              ubyte reserved;
              ushort structSize;
              ushort numSettings;
              Count = numSettings;
              ushort reserved2;
              unsigned int reserved3[2];
            }HEADER <open=true>;
            break;
        case "zeon": //ZeroOne
